<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/Cet_Physics_test"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="5dp"
		android:layout_marginTop="8dp"
		android:layout_marginRight="5dp"
		android:layout_marginBottom="3dp"
		android:orientation="vertical"
		android:layout_gravity="center_horizontal|center_vertical">
		<LinearLayout
			android:id="@+id/linear16"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview2"
				android:focusable="false"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_marginLeft="5dp"
				android:layout_marginTop="2dp"
				android:layout_marginBottom="2dp"
				android:src="@drawable/deadline"
				android:scaleType="fitCenter" />
			<TextView
				android:id="@+id/textview8"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="LIVE TEST"
				android:textSize="20sp"
				android:textStyle="bold"
				android:textColor="#000000"
				android:layout_gravity="center_horizontal|center_vertical" />
			<ImageView
				android:id="@+id/imageview1"
				android:focusable="false"
				android:layout_width="10dp"
				android:layout_height="10dp"
				android:src="@drawable/red_dot"
				android:scaleType="fitCenter"
				android:layout_gravity="center_horizontal|center_vertical" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear17"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview9"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="Test Name:"
				android:textSize="18sp"
				android:textStyle="bold"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/testname"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="CET Physics"
				android:textSize="18sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear18"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview10"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="Time:"
				android:textSize="18sp"
				android:textStyle="bold"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/testdate"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="8:00 to 8:20"
				android:textSize="18sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear19"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview11"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="Duration:"
				android:textSize="18sp"
				android:textStyle="bold"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/Testtime"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="20 minutes test"
				android:textSize="18sp"
				android:textColor="#000000" />
		</LinearLayout>
		<Button
			android:id="@+id/button16"
			android:layout_width="wrap_content"
			android:layout_height="40dp"
			android:layout_marginLeft="2dp"
			android:layout_marginTop="2dp"
			android:layout_marginRight="7dp"
			android:layout_marginBottom="7dp"
			android:padding="8dp"
			android:text="Start test"
			android:textSize="16sp"
			android:textStyle="bold"
			android:textColor="#FFFFFF"
			android:layout_gravity="right|center_vertical" />
	</LinearLayout>
</LinearLayout>
